<h1>Remove Player<h1>

<%= form_for(@session) do |f| %>
        <h4><%= f.label :user, "Select Player to Remove:" %><h4>
        <%= f.collection_select(:user_id, User.all, :id, :username) %>
        <%= f.hidden_field :session_id, :value => @session.id  %>
        <%= f.submit "Remove Player" %>
    <% end %>